<view><view class="header"><block a:for="{{filterByList}}" a:for-item="target" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['handleSelect',[index]]]]]}}" class="{{(('target')+' '+((target.selected)?'on':''))}}" onTap="__e">{{''+target.text+''}}</view></block></view><view class="place"></view><view class="goods-list"><view class="product-list"><block a:for="{{goodsList}}" a:for-item="goods" a:for-index="__i0__" a:key="goods_id"><view data-event-opts="{{[['tap',[['handleGoods',['$0'],[[['goodsList','goods_id',goods.goods_id]]]]]]]}}" class="product" onTap="__e"><image mode="widthFix" src="{{goods.img}}"></image><view class="name">{{goods.name+"}"}}</view><view class="info"><view class="price"><text>￥</text>{{''+goods.price+''}}</view><view class="slogan">{{goods.slogan+''}}<text>人付款</text></view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>