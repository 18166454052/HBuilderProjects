<view><page-status vue-id="1" onVueInit="__l"></page-status><page-header vue-id="2" onVueInit="__l"></page-header><view class="category-list"><scroll-view class="left" scroll-y="true"><block a:for="{{categoryList}}" a:for-item="category" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['handleCategory',[index]]]]]}}" class="{{(('row')+' '+((index==showCategoryIndex)?'on':''))}}" onTap="__e"><view class="text"><view class="block"></view>{{''+category.title+''}}</view></view></block></scroll-view><scroll-view class="right" scroll-y="true"><block a:for="{{categoryList}}" a:for-item="category" a:for-index="index" a:key="id"><view hidden="{{!(index==showCategoryIndex)}}" class="category"><view class="banner"><image src="{{category.banner}}"></image></view><view class="list"><block a:for="{{category.list}}" a:for-item="item" a:for-index="i" a:key="i"><view data-event-opts="{{[['tap',[['handleCategoryList',['$0'],[[['categoryList','id',category.id],['list','',i]]]]]]]}}" class="box" onTap="__e"><image src="{{'/static/img/category/list/'+item.img}}"></image><view class="text">{{item.name}}</view></view></block></view></view></block></scroll-view></view></view>